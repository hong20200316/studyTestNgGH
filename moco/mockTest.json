[
  {
    "description":"mock测试例子",
    "request":{
      "uri":"/demo"
    },
    "response":{
      "text":"第一个moco测试结果"
    }
  },

  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"get请求的返回结果"
    }
  },
  {
    "description":"模拟一个带参数的get请求",
    "request":{
      "uri":"/getdemoPara",
      "method":"get",
      "queries":{
        "name":"liyanhong",
        "age":"30"
      }
    },
    "response":{
      "text":"我是一个带参数的get请求！"
    }

  },
  {
    "description":"这是一个post请求",
    "request":{
      "uri":"/postdemo",
      "method":"post"
    },
    "response":{
      "text":"这是一个post请求"
    }
  },

  {
    "description":"这是一个带参数的post请求",
    "request":{
      "uri":"/postdemoPara",
      "method":"post",
      "forms":{
        "name":"ding",
        "age":"15"
      }
    },
    "response":{
      "text":"这是一个带参数的post请求"
    }
  },
  {
    "description":"带cookies信息的get请求",
    "request":{
      "uri":"/getCookies",
      "method":"get",
      "cookies":{
        "token":"1234"
      }
    },
    "response":{
      "text":"这是一个带cookies的get请求！！"
    }
  },
  {
    "description":"带cookies和请求参数信息的post请求,注意post请求参数可以用forms和json两种格式",
    "request":{
      "uri":"/postCookiePara",
      "method":"post",
      "cookies":{
        "login":"111"
      },
      "json":{
        "name":"yanhong",
        "age":"5"
      }
    },
    "response":{
      "text":"这是一个带cookies信息和参数信息的post请求"
    }
  },
  {
    "description":"带header信息的post请求",
    "request":{
      "uri":"/postHeader",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"hui",
        "age":"14"
      }
    },
    "response":{
      "status":"200",
      "json":{
        "name":"headername",
        "sha":"json"
      }
    }
  },
  {
    "description":"重定向",
    "request":{
      "uri":"/demo/Tobaidu",
      "method":"get"
    },
    "redirectTo":"http://www.baidu.com"
  }

]